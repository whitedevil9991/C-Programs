#include<stdio.h>
int main()
{
    int a,b,c;
    printf("enter three numbers :");
    scanf("%d%d%d",&a,&b,&c);
    if(a>b)&&(a>c)
    {
        printf("%d is the greatest",&a);
    }
    if(b>a)&&(b>c)
    {
        printf("second number is the greatest");
    }
    if(c>a)&&(c>b)
    {
        printf("third number is the greatest");
    }
    if(a==b)&&(b==c)&&(c==a)
    {
        printf("all are equal");
    }
}
