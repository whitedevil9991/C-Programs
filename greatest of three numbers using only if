#include <stdio.h>

int main() 
{
    int a,b,c;
    printf("enter three numbers :");
    scanf("%d%d%d",&a,&b,&c);
    if(a>b)
    { 
        if(a>c)
        {
            printf("first number is greater");
        }
    }
          if(b>a)
          {
            if(b>c)
            {
                printf("second number is greater");
            }
          }
            if(c>a)
            {
                if(c>b)
                {
                    printf("third number is greater");
                }
            }
            if(a==b)
            {
             if(a==c)
             {
                 printf("all are equal");
             }
            }
          

}
