#include<stdio.h>
int main()
{
    int arr[20],n,j,i,x,low,high,mid;
    low=0;
    high=n-1;
    do
    {
        mid=(low+high)/2;
        if(x>arr[mid])
        {
            high=mid+1;
        }
        else if(x>arr[mid])
        {
            low=mid+1;
        }
        else
        {
        printf("number found");
        break;
        }
        while(low<=high)
        if(low>high)     
        printf("number not found");
    }
}
